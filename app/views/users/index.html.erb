<%= render 'books/list', book: @book, user: @user%>
		<div class="col-xs-9 col-xs-offset-1">
			<h2>Users</h2>
			 <table>
			 	<thead>
			 		<div class="row book">
			 			<div class="col-xs-4">image</div>
			 			<div calss="col-xs-4">name</div>
			 		</div>
			 	</thead>
			 	<tbody>
			 		<% @users.each do |user| %>
			 		 <div class="row book">
			 		 	<div class="col-xs-4">
			 		 		<%= attachment_image_tag user, :profile_image, size: "40x40", class: "img-circle pull-left profile-thumb", fallback: "no_image.jpg"%>
			 		 	</div>
			 		 	<div class="col-xs-4">
			 		 	<%= user.name %>
			 		    </div>
			 		    <div class="col-xs-4">
			 		 	<%= link_to "show", user_path(user.id) %>
			 		    </div>
			 		 </div>
			 		 <% end %>
			 	</tbody>
		     </table>
		</div>
	</div>
</div>